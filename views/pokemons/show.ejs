<div class="container">
  <h1 class="text-center">Your Favorite Pokemon</h1>
  <p class="text-center">
    <a href="/">Home</a>
  </p>

  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <h2><a href="/pokemon/<%= pokemon.name %>"><%= pokemon.name %></h2>
      <form method="POST" action="/pokemon">
        <input type="hidden" name="name" value="<%= pokemon.name %>">
      </form>
    </div>
  <% }); %>
</div>
